<script type="text/javascript">

var osatleast = document.getElementById("osatleast");
var osesDropdown = document.getElementById("idOS");
var osVersionsDropdown = document.getElementById("osdetails-ver");

var makingAListFor = 0;
var versionsForOS = {};

var l = osVersionsDropdown.options;

for (var i = 0; i < l.length; i++) {
	if (l[i].disabled) {
		makingAListFor = l[i].value;
		versionsForOS[makingAListFor] = [];
	} else {
		versionsForOS[makingAListFor].push([l[i].value, l[i].text]);
	}
};

function makeChildOption(value, text) {
	var x = document.createElement("option");
	x.value = value;
	x.text = text;
	x.innerText = text;

	return x;
}

function removeChildren(x) {
	while (x.hasChildNodes()) {
		x.removeChild(x.lastChild);
	}
}

removeChildren(osVersionsDropdown);
osVersionsDropdown.appendChild(makeChildOption("0", "Any"));

function updateOSVersionsDropdown() {
	var osPicked = osesDropdown.options[osesDropdown.selectedIndex].value;

	removeChildren(osVersionsDropdown);
	osVersionsDropdown.appendChild(makeChildOption("0", "Any"));

	if (typeof versionsForOS[osPicked] == "undefined" || versionsForOS[osPicked].length == 0) {
		osatleast.style.display = "none";
		return;
	}

	for (var i = 0; i < versionsForOS[osPicked].length; i++) {
		var osVersionId = versionsForOS[osPicked][i][0];
		var osVersionName = versionsForOS[osPicked][i][1];

		osVersionsDropdown.appendChild(makeChildOption(osVersionId, osVersionName));
	};

	osatleast.style.display = "";
}

var lastSelectedOSVersion = osesDropdown.selectedIndex;

function updateHistoryState() {
	var state = "osesDropdown=" + osesDropdown.selectedIndex + ";";
	state += "osVersionsDropdown=" + osVersionsDropdown.selectedIndex + ";"
	
	history.replaceState(state, "");
}

function parseHistoryState() {
	var state = history.state;
	var res = {};
	
	var elems = state.split(";");
	for (var i = 0; i < elems.length; i++) {
		var el = elems[i].split("=");
		res[el[0]] = el[1];
	}
	
	return res;
}

if (history.state) {
	var state = parseHistoryState();
	osesDropdown.selectedIndex = state["osesDropdown"];
	updateOSVersionsDropdown();
	osVersionsDropdown.selectedIndex = state["osVersionsDropdown"];
}

function handleChangeAndKeyPress() {
	if (osesDropdown.selectedIndex != lastSelectedOSVersion) {
		lastSelectedOSVersion = osesDropdown.selectedIndex;
		updateOSVersionsDropdown();
		
		updateHistoryState();
	}
}

if (osesDropdown.addEventListener) {
	osesDropdown.addEventListener("click", updateOSVersionsDropdown, false);
	osesDropdown.addEventListener("keypress", handleChangeAndKeyPress, false);
	osesDropdown.addEventListener("change", handleChangeAndKeyPress, false);
	osVersionsDropdown.addEventListener("keypress", updateHistoryState, false);
	osVersionsDropdown.addEventListener("change", updateHistoryState, false);
} else {
	osesDropdown.attachEvent("onclick", updateOSVersionsDropdown);
	osesDropdown.attachEvent("onchange", updateOSVersionsDropdown);
	setTimeout("osesDropdown.options[0].selected = true", 100);
} 
</script> 

